{
  "name": "vscode-server-tool",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./node_modules/.cache ./server/dist ./build ./public/build \"./app/styles/**/*.css\"",
    "prebuild": "yarn run clean",
    "build:remix": "cross-env NODE_ENV=production dotenv -e .env remix build",
    "build:server": "node ./config/build-server.js",
    "build:prisma": "node ./config/build-prisma.js",
    "build": "yarn run build:remix && yarn run build:server && yarn run build:prisma",
    "predev": "cd docker && docker build --tag vscode-server-tool -f Dockerfile.vscode .",
    "dev": "concurrently \"yarn run dev:server\" \"yarn run dev:remix\"",
    "dev:server": "cross-env NODE_ENV=development node --inspect ./index.js",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "start": "cross-env NODE_ENV=production node --require ./node_modules/dotenv/config ./index.js",
    "postinstall": "remix setup node",
    "prisma:generate": "prisma generate",
    "prisma:db:push": "prisma db push",
    "prisma:db:seed": "prisma db seed",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:studio": "prisma studio",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|gql|graphql|mdx|vue)\"",
    "typecheck": "tsc -b",
    "validate": "concurrently \"yarn run lint\" \"yarn run typecheck\" \"yarn run build\""
  },
  "prisma": {
    "seed": "node config/runfile prisma/seed.ts"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.6",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "11",
    "@emotion/server": "11",
    "@emotion/styled": "11",
    "@prisma/client": "^3.11.1",
    "@remix-run/express": "^1.3.4",
    "@remix-run/node": "1.3.4",
    "@remix-run/react": "^1.3.4",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dockerode": "3.2.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.31",
    "express": "^4.17.1",
    "framer-motion": "6",
    "fs-extra": "^10.0.1",
    "history": "^5.2.0",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "remix": "^1.1.3",
    "simple-git": "^3.4.0",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@remix-run/dev": "^1.3.4",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/dockerode": "3.2.0",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/ws": "^8.5.3",
    "autoprefixer": "10.4.2",
    "babel-loader": "^8.2.3",
    "chromatic": "6.4.1",
    "concurrently": "7.0.0",
    "constructs": "^10.0.54",
    "dotenv-cli": "^5.1.0",
    "esbuild-register": "3.1.2",
    "eslint": "^7.32.0",
    "eslint-config-kentcdodds": "^19.2.0",
    "glob": "^7.2.0",
    "nodemon": "^2.0.15",
    "prisma": "^3.11.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "server-build",
    "build",
    "public/build",
    "*.ignored/",
    "*.ignored.*"
  ],
  "engines": {
    "node": "16"
  }
}
